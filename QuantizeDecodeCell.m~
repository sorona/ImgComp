function [C.A , C.GAMMA]=QuantizeDecodeCell(Cq,bins)
    % Quantization Decode GAMMA
    Size   = Cq.GAMMA.OriginalSize; 
    DC     = Cq.GAMMA.Dc;
    Max    = Cq.GAMMA.Max;
    index  = Cq.GAMMA.Qindex;
    
    codebook = (-bins/2:1:bins/2-1);
    quant    = (codebook(index+1));
    GAMMAvec       = quant/(bins/2)*Max+DC;
    GAMMAr   = vec2GAMMA(GAMMAvec,par);
end